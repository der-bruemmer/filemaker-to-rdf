<?xml version="1.0" encoding="UTF-8" ?>
<mappings>
	<valency>
		<namespaces>
			<namespace>
				dcterms=http://purl.org/dc/terms/
			</namespace>
			<namespace>
				gold=http://linguistics-ontology.org/gold/2010/
			</namespace>
			<namespace>
				rdfs=http://www.w3.org/2000/01/rdf-schema#
			</namespace>
			<namespace>
				val=http://www.eva.mpg.de/lingua/valency/vocabulary/
			</namespace>
		</namespaces>
<!-- Languages -->
		<table>
			<Languages>
				<resource>
					<property>
						rdfs:class
					</property>
					<resourceUri>
						http://linguistics-ontology.org/gold/2010/language
					</resourceUri>
				</resource>
				<primary>
					<Language_ID>
						<URI>
							http://www.eva.mpg.de/lingua/valency/language/$Language_ID
						</URI>
					</Language_ID>
				</primary>
				<column>
					<Language_name>
						<property>
							dcterms:title
						</property>
						<property>
							rdfs:label
						</property>
					</Language_name>
				</column>
				<column>
					<ISO_code>
						<property>
							val:isoCode
						</property>
						<resource>
							<property>
								dcterms:relation
							</property>
							<resourceUri>
								http://www.sil.org/iso639-3/documentation.asp?id=$ISO_code
							</resourceUri>
						</resource>
					</ISO_code>
				</column>
				<column>
					<Language_variety>
						<property>
							val:languageVariety
						</property>
					</Language_variety>
				</column>
				<column>
					<Record_creation_date>
						<property>
							dcterms:created
						</property>
					</Record_creation_date>
				</column>
				<column>
					<Record_creator>
						<property>
							dcterms:creator
						</property>
					</Record_creator>
				</column>
				<column>
					<Record_modification_date>
						<property>
							dcterms:modified
						</property>
					</Record_modification_date>
				</column>
				<column>
					<Record_modifier>
						<property>
							val:modifier
						</property>
					</Record_modifier>
				</column>
			</Languages>
			<!--<language_family>
				<URI>
