					</Record_modifier>
				</column>
				<foreign table='Verb_Examples'>
					<column primary='Verb_ID' foreign='Example_ID'>
						<resource>
							<property>
								val:hasExample
							</property>
							<resourceUri>
								http://www.eva.mpg.de/lingua/valency/example/$Example_ID
							</resourceUri>
						</resource>
					</column>
				</foreign>
				<foreign table='Verb_Meanings'>
					<column primary='Verb_ID' foreign='Meaning_ID'>
						<resource>
							<property>
								val:hasMeaning
							</property>
							<resourceUri>
								http://www.eva.mpg.de/lingua/valency/meaning/$Meaning_ID
							</resourceUri>
						</resource>
					</column>
